{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [false, "spaces", 4],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "no-console": false,
    "member-access": false,
    "semicolon": [
        false,
        "always"
     ],
    "quotemark": [false, "single"]
  }
}
